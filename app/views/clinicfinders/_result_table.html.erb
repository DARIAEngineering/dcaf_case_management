<h3><%= title %></h3>
<table class="table">
  <thead>
    <% labels.each do |label| %>
      <th><%= label %></th>
    <% end %>
  </thead>
  <tbody>
    <% clinics.each do |clinic| %>
      <tr>
        <% clinic_fields.each do |field| %>
          <td><%= parse_clinicfinder_field clinic, field %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
