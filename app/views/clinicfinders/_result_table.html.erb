<h3><%= title %></h3>

<table class="table">
  <thead>
    <% clinic_fields.each do |label| %>
      <th><%= t "mongoid.attributes.clinic.#{label}" %></th>
    <% end %>
  </thead>

  <tbody>
    <% clinics.each do |clinic| %>
      <tr>
        <% clinic_fields.each do |field| %>
          <td><%= parse_clinicfinder_field clinic, field %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
