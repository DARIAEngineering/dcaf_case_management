$('.clinic-finder-results').empty();

<% if @nearest.any? %>
  $(".clinic-finder-results").append("<%= escape_javascript(render partial: 'clinicfinders/result_table', locals: { title: 'Nearest Clinics', labels: ['Name', 'Distance'], clinic_fields: [:name, :distance], clinics: @nearest }) %>");
<% end %>

<% if @cheapest.any? %>
  $(".clinic-finder-results").append("<%= escape_javascript(render partial: 'clinicfinders/result_table', locals: { title: 'Most Affordable Clinics', labels: ['Name', 'Cost'], clinic_fields: [:name, :cost], clinics: @cheapest }) %>");
<% end %>
