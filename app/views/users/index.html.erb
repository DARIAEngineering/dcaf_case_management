<h1>User Account Management</h1>
<div class="row" style="padding-top: 10px; padding-bottom: 20px;">
  <div class="col-md-8">
    <%= link_to "Add New User", new_user_path, class: "btn btn-primary" %>
    <%= link_to "Reset Password (WIP)", "#", class: "btn btn-primary" %>
    <button type="button" class="btn btn-primary">Toggle Lock (WIP)</button>

  </div>
  <div class="col-md-4">
    <div style="float: right;">
      <%= render partial: 'users/search_form', locals: { autosortable: true } %>

    </div>
  </div>
</div>
<%= render partial: 'users/users_table', locals: { users: @users, autosortable: true } %>
